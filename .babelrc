{
  "presets": ["react", "es2015"],
  "env": {
    "development": {
    "plugins": [
      ["react-transform", {
         "transforms": [{
           "transform": "react-transform-hmr",
           "imports": ["react"],
           "locals": ["module"]
        }]
      }],
      ["transform-class-properties", { "spec": true }],
      ["import", { "libraryName": "antd-mobile",  "style": "css"  }]
     ]
    }
  }
}